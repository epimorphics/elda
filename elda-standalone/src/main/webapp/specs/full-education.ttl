#ENDPOINTS

# Ontology endpoints [cloned and edited from Stuart's
# bathing-water endpoint].

so:ontology-selector api:where
"""
{ ?item a owl:Ontology } 
UNION {?item a skos:ConceptScheme }
UNION {?item a ?type. ?type rdfs:subClassOf skos:Concept}
FILTER (regex(STR(?item),?schemeRegex))
""";     
    api:orderBy "str(?item)";
    .

so:scheme-selector
     api:where """
{?item a owl:Ontology} UNION
{?item a qb:DimensionProperty} UNION
{?item a qb:MeasureProperty} UNION
{?item a qb:AttributeProperty} UNION
{?item a rdf:Property} UNION
{?item a owl:ObjectProperty} UNION
{?item a owl:DatatypeProperty} UNION
{?item a rdfs:Class} UNION
{?item a rdfs:Datatype } UNION
{?item a owl:Class} UNION
{?item a owl:Restriction} UNION
{?item a owl:DatatypeProperty} UNION
{?item a owl:ObjectProperty} UNION
{?item a skos:Concept} UNION
{?item a skos:ConceptScheme} UNION
{?item a ?type. ?type rdfs:subClassOf skos:Concept}
FILTER (regex(STR(?item),?schemeRegex))
""";
     api:orderBy "str(?item)";
     .
    
#
# The /item endpoint requires a query parameter whose value
# is a URI; this is the single selected ?item. These URIs
# are typically constructed by the Velocity renderer when
# a resource is clicked on for a dataset-relative link. 
#

so:about a api:ListEndpoint
    ; rdfs:label "About"
    ; api:uriTemplate "/about?resource={uri}"
    ; api:variable [api:name "uri"; api:type rdfs:Resource]
    ; api:selector 
        [ api:where "?item ?p ?o. FILTER(?item = ?uri)"
        ; api:sort "label"
        ]
    ; api:defaultViewer api:labelledDescribeViewer
    ; api:viewer
        spec:viewerShort
        , spec:viewerMedium
        , spec:schemeViewer
        , spec:viewerProvision
        , spec:viewerLocation
        , spec:viewerPerformance
        , spec:viewerAdmin
        , spec:definitionViewer
    .
    
so:scheme-def a api:ListEndpoint;
	rdfs:comment "Returns a list of vocabularies organised under http://education.data.gov.uk/def/"@en;
	api:viewer    api:describeViewer, spec:schemeViewer, api:basicViewer;
	api:uriTemplate  "/def";
	api:variable [ api:name "schemeRegex"; api:value "^http://education.data.gov.uk/def/" ];
	api:selector so:ontology-selector ;
	api:defaultViewer spec:schemeViewer ;
	.

so:scheme a api:ListEndpoint ;
	rdfs:comment "Returns a paged list of vocabulary terms from a given vocabulary."@en;
	api:uriTemplate "/def/{scheme}" ;
	api:variable [ api:name "schemeRegex"; api:value "^http://education.data.gov.uk/def/{scheme}/" ];
	api:selector so:scheme-selector ;
	api:viewer spec:definitionViewer, api:describeViewer, api:basicViewer ;
	api:defaultViewer spec:schemeViewer ;
	.

so:scheme-ns a api:ItemEndpoint ;
   rdfs:comment "Returns information about the vocabulary."@en;
   api:uriTemplate "/def/{scheme}/" ;
   api:itemTemplate "http://education.data.gov.uk/def/{scheme}/" ;
   api:viewer spec:schemeViewer, api:describeViewer, api:basicViewer;
   api:defaultViewer spec:schemeViewer;
   .

spec:schemeViewer a api:Viewer ;
	api:name "scheme" ;
	api:properties "type,comment,label,altLabel,note,scopeNote,topConcept.prefLabel,comment" ;
	.

so:subclass-selector
     api:where " ?item rdfs:subClassOf ?superclass . " ;
     api:orderBy "STR(?item)";
     .

so:subclasses a api:ListEndpoint ;
   rdfs:comment "Lists defined subclasses of a given vocabulary (class) term."@en;
   api:uriTemplate "/def/{scheme}/{term}/subclass" ;
   api:variable [
     api:name "superclass" ;
     api:value "http://education.data.gov.uk/def/{scheme}/{term}" ;
     api:type rdfs:Resource ;
   ] ;
   api:selector so:subclass-selector ;
   api:viewer spec:definitionViewer, api:describeViewer, api:basicViewer;
   api:defaultViewer spec:definitionViewer ;
   .

so:instances-selector
     api:where " ?item a ?class . " ;
     api:orderBy "STR(?item)";
     .

so:instances a api:ListEndpoint ;
   rdfs:comment "Lists known instances of a given vocabulary (class) term."@en;
   api:uriTemplate "/def/{scheme}/{term}/instance" ;
   api:variable [
     api:name "class" ;
     api:value "http://education.data.gov.uk/def/{scheme}/{term}" ;
     api:type rdfs:Resource ;
   ] ;
   api:selector so:instances-selector;
   api:viewer spec:definitionViewer, api:describeViewer, api:basicViewer;
   api:defaultViewer api:describeViewer ;
   .

so:properties-selector
     api:where "{ select ?thing where { ?thing a ?class } limit 1000} ?thing ?item []." ;
     api:orderBy "STR(?item)";
     .

so:properties a api:ListEndpoint ;
   rdfs:comment "Lists properties used in the dataset to describe instances of a given vocabulary (class) term."@en;
   api:uriTemplate "/def/{scheme}/{term}/property" ;
   api:variable [
     api:name "class" ;
     api:value "http://education.data.gov.uk/def/{scheme}/{term}" ;
     api:type rdfs:Resource ;
   ] ;
   api:selector so:properties-selector;
   api:viewer spec:definitionViewer, api:describeViewer, api:basicViewer;
   api:defaultViewer api:describeViewer ;
   .


