      <query id="_:query-1039">
        <value datatype="string">PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
PREFIX school: &lt;http://education.data.gov.uk/def/school/&gt;
PREFIX geo: &lt;http://www.w3.org/2003/01/geo/wgs84_pos#&gt;
PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
PREFIX school: &lt;http://education.data.gov.uk/def/school/&gt;
CONSTRUCT {?item school:establishmentNumber ?___1 .
?item geo:long ?___2 .
?item school:gender ?___3 .
?___3 rdfs:label ?___4 .
?item geo:lat ?___5 .
?item school:typeOfEstablishment ?___6 .
?___6 rdfs:label ?___7 .
?item school:phaseOfEducation ?___8 .
?___8 rdfs:label ?___9 .
?item rdfs:label ?___10 .
?item school:religiousCharacter ?___11 .
?___11 rdfs:label ?___12 .
?item school:uniqueReferenceNumber ?___13 .

} WHERE {
  {SELECT ?item
    WHERE {
    ?___establishmentStatus_0 rdfs:label "Open" .
    ?item school:establishmentStatus ?___establishmentStatus_0 .
    ?item rdf:type school:School .
    }  OFFSET 0 LIMIT 10
}{ ?item school:establishmentNumber ?___1 . } 
UNION { ?item geo:long ?___2 . } 
UNION {{ ?item school:gender ?___3 . }  OPTIONAL {
  { ?___3 rdfs:label ?___4 . } 
}}
UNION { ?item geo:lat ?___5 . } 
UNION {{ ?item school:typeOfEstablishment ?___6 . }  OPTIONAL {
