}}
UNION { ?item rdfs:label ?___10 . } 
UNION {{ ?item school:religiousCharacter ?___11 . }  OPTIONAL {
  { ?___11 rdfs:label ?___12 . } 
}}
UNION { ?item school:uniqueReferenceNumber ?___13 . } 

}</value>
